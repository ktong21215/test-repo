{
  "name": "lendage-config-project",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^12.8.3",
    "@types/jest": "^26.0.22",
    "@types/node": "^12.20.7",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-html-reporter": "^0.7.4",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest-axe": "^4.1.0",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.33",
    "node-sass": "4.14.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "stylelint-formatter-html": "^1.0.1",
    "web-vitals": "^1.1.1"
  },
  "scripts": {
    "build": "react-scripts build",
    "build-storybook": "build-storybook -s public",
    "storybook": "start-storybook -p 6006 -s public",
    "dev": "start-storybook -p 6006",
    "start": "react-scripts start",
    "eslint": "eslint \"./src/*.{js,jsx,ts,tsx}\"",
    "eslint:report": "eslint \"./src/*.{js,jsx,ts,tsx}\" -f node_modules/eslint-html-reporter/reporter.js -o ./reports/EslintReport.html",
    "eslint:fix": "eslint --fix \"./src/*.{js,jsx,ts,tsx}\"",
    "lint": "npm run eslint && npm run stylelint",
    "lint:fix": "npm run eslint:fix && npm run stylelint:fix",
    "stylelint": "stylelint \"./src/*.{css,scss}\"",
    "stylelint:report": "stylelint -o ./reports/StyleLintReport.html --aei --custom-formatter node_modules/stylelint-formatter-html \"./src/*.{css,scss}\"",
    "stylelint:fix": "stylelint \"./src/*.{css,scss}\" --fix",
    "test": "jest --coverage --watchAll=false",
    "test:watch": "jest --watch"
  },
  "keywords": [
    "react",
    "typescript",
    "storybook",
    "jest"
  ],
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@sindeo/lendage-npm-style-guide": "^2.0.12",
    "@storybook/addon-a11y": "^6.1.14",
    "@storybook/addon-actions": "^6.2.8",
    "@storybook/addon-essentials": "^6.2.8",
    "@storybook/addon-links": "^6.2.8",
    "@storybook/node-logger": "^6.2.8",
    "@storybook/preset-create-react-app": "^3.1.7",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.2.8",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^10.4.8",
    "@types/jest": "^26.0.20",
    "@types/jest-axe": "^3.5.1",
    "@types/react-test-renderer": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "8.1.0",
    "css-loader": "^4.2.1",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.7.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest-sonar-reporter": "^2.0.0",
    "normalize.css": "^8.0.1",
    "prettier": "^2.2.1",
    "react-html-parser": "^2.0.2",
    "react-is": "^17.0.1",
    "react-test-renderer": "^17.0.1",
    "rollup": "^2.45.2",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-typescript2": "^0.30.0",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "stylelint": "^13.12.0",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.19.0",
    "ts-jest": "^26.5.4",
    "typescript": "^4.2.4"
  }
}
